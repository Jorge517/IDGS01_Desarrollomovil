<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App3.MainPage">

    <StackLayout>

        <Frame BackgroundColor="#6E33FF" Padding="8">
            <Label Text="UTEQ" HorizontalTextAlignment="Center" TextColor="White" FontSize="36"/>
        </Frame>
        <Image Source="https://imagenes.milenio.com/7rLsGj1tHeLJWPE-XEgS_ItoTL8=/958x596/https://www.milenio.com/uploads/media/2021/03/22/la-vida-de-homero-simpson.jpg" />
        <Entry Text="{Binding Nombre}"></Entry>
        <Entry Text="{Binding AppP}"></Entry>
        <Entry Text="{Binding AppM}"></Entry>
        <Label Text="{Binding NombreCompleto}" HorizontalTextAlignment="Center" FontSize="18"></Label>

    </StackLayout>

</ContentPage>
